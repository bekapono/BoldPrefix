{
  "manifest_version": 3,
  "name":            "BoldPrefix",
  "version":         "1.0",
  "description":     "Make the front half of each word bold",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup":  "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["content.js", "anticontent.js"],
      "matches":   ["<all_urls>"]
    }
  ]
}
